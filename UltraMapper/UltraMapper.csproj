<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netstandard2.0;net45;net46;net47</TargetFrameworks>
    <AutoGenerateBindingRedirects>true</AutoGenerateBindingRedirects>
    <Authors>Mauro Sampietro</Authors>
    <Copyright>2020</Copyright>
    <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
    <PackageVersion>1.0.0.0</PackageVersion>
    <PackageProjectUrl>https://maurosampietro.github.io/UltraMapper/</PackageProjectUrl>
    <RepositoryUrl>https://github.com/maurosampietro/UltraMapper</RepositoryUrl>
    <Description>A .NET object-mapper useful to deep-copy an object, or transform (map) an object of a certain type to another type.</Description>
    <PackageReleaseNotes></PackageReleaseNotes>
    <PackageTags>map mapper object deep copy</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="UltraMapper.csproj.old" />
  </ItemGroup>

  <ItemGroup>
    <AssemblyAttribute Include="System.Runtime.CompilerServices.InternalsVisibleTo">
      <_Parameter1>UltraMapper.Tests</_Parameter1>
    </AssemblyAttribute>
    <AssemblyAttribute Include="System.Runtime.CompilerServices.InternalsVisibleTo">
      <_Parameter1>UltraMapper.CommandLine</_Parameter1>
    </AssemblyAttribute>
    <AssemblyAttribute Include="System.Runtime.CompilerServices.InternalsVisibleTo">
      <_Parameter1>UltraMapper.Csv</_Parameter1>
    </AssemblyAttribute>
    <AssemblyAttribute Include="System.Runtime.CompilerServices.InternalsVisibleTo">
      <_Parameter1>UltraMapper.Parsing</_Parameter1>
    </AssemblyAttribute>
  </ItemGroup>
</Project>
